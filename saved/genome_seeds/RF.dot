CPPN { // Solves C1 mazes (for a resolution of 7 -> Resolution input to the CPPN?)
    x0; y0; z0; x1; y1; z1; l; b;

    W [bsgm];
    L [step];
    B [id];

    // abs(x0+x1)
    0 [abs];
    x0 -> 0 [1];
    x1 -> 0 [1];

    // step(H0-1.4)
    1 [step];
    0 -> 1 [1];
    b -> 1 [-1.4];

    // abs(z0+z1)
    2 [abs];
    z0 -> 2 [1];
    z1 -> 2 [1];

    // step(H2-1.4)
    3 [step];
    2 -> 3 [1];
    b -> 3 [-1.4];

    // step(H2+H3-0.5)
    4 [step];
    1 -> 4 [1];
    3 -> 4 [1];
    b -> 4 [-.5];

    // step(abs(x0)-.33)
    5 [abs];
    6 [step];
    x0 -> 5 [1];
    5 -> 6 [1];
    b -> 6 [-.33];

    // step(abs(z0)-.33)
    7 [abs];
    8 [step];
    z0 -> 7 [1];
    7 -> 8 [1];
    b -> 8 [-.33];

    // sin(H6+H8);
    9 [sin];
    6 -> 9 [1];
    8 -> 9 [1];

    // W = bsgm(-H4+0.1*H9)
    4 -> W [-1];
    9 -> W [.05];

    // ----

    // abs(.5x0+x1)
    10 [abs];
    x0 -> 10 [-.5];
    x1 -> 10 [1];

    // abs(.5z0+z1)
    11 [abs];
    z0 -> 11 [-.5];
    z1 -> 11 [1];

    // step(H10-.45)
    12 [step];
    10 -> 12 [1];
    b -> 12 [-.45];

    // step(H11-.45)
    13 [step];
    11 -> 13 [1];
    b -> 13 [-.45];

    // step(H10-.95)
    14 [step];
    10 -> 14 [1];
    b -> 14 [-.95];

    // step(H11-.95)
    15 [step];
    11 -> 15 [1];
    b -> 15 [-.95];

    // step(H10-H13-H14-.7)
    16 [step];
    10 -> 16 [1];
    13 -> 16 [-1];
    14 -> 16 [-1];
    b -> 16 [-.7];

    // step(H11-H12-H15.7)
    17 [step];
    11 -> 17 [1];
    12 -> 17 [-1];
    15 -> 17 [-1];
    b -> 17 [-.7];

    // L = step(H11+H13-1.1l)
    16 -> L [1];
    17 -> L [1];
    b -> L [-.5];
//     l -> L [-1.275];
}
