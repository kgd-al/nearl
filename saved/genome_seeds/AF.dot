CPPN { // Solves C1 mazes (for a resolution of 7 -> Resolution input to the CPPN?)
    x0; y0; z0; x1; y1; z1; l; b;

    W [bsgm];
    L [step];
    B [id];

    // abs(x0+x1)
    0 [abs];
    x0 -> 0 [1];
    x1 -> 0 [1];

    // step(H0-1.4)
    1 [step];
    0 -> 1 [1];
    b -> 1 [-1.4];

    // abs(z0+z1)
    2 [abs];
    z0 -> 2 [1];
    z1 -> 2 [1];

    // step(H2-1.4)
    3 [step];
    2 -> 3 [1];
    b -> 3 [-1.4];

    // step(H2+H3-0.5)
    4 [step];
    1 -> 4 [1];
    3 -> 4 [1];
    b -> 4 [-.5];

    // step(abs(x0)-.33)
    5 [abs];
    6 [step];
    x0 -> 5 [1];
    5 -> 6 [1];
    b -> 6 [-.33];

    // step(abs(z0)-.33)
    7 [abs];
    8 [step];
    z0 -> 7 [1];
    7 -> 8 [1];
    b -> 8 [-.33];

    // sin(H6+H8);
    9 [sin];
    6 -> 9 [1];
    8 -> 9 [1];

    // W = bsgm(-H4+0.1*H9)
    4 -> W [-1];
    9 -> W [.05];

    // ----

    // step(abs(.5x0+x1)-.7)
    10 [abs];
    11 [step];
    x0 -> 10 [.5];
    x1 -> 10 [1];
    10 -> 11 [1];
    b -> 11 [-.7];

    // step(abs(.5z0+z1)-.7)
    12 [abs];
    13 [step];
    z0 -> 12 [.5];
    z1 -> 12 [1];
    12 -> 13 [1];
    b -> 13 [-.7];

    // L = step(H11+H13-1.1l)
    11 -> L [1];
    13 -> L [1];
    l -> L [-1.275];
}
