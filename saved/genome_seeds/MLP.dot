CPPN { // Solves C1 mazes (for a resolution of 7 -> Resolution input to the CPPN?)
    x0; y0; z0; x1; y1; z1; l; b;

    W [bsgm];
    L [step];
    B [id];

    // Distance
    l -> L [-1];
    b -> L [.37];

    // MLP
    3 [abs];
    4 [abs];
    5 [abs];
    6 [abs];
    7 [sin];
    8 [sin];
    x0 -> 3 [1];
    x1 -> 4 [1];
    z0 -> 5 [1];
    z1 -> 6 [1];
    3 -> 7 [1.57];
    4 -> 7 [1.57];
    5 -> 8 [1.57];
    6 -> 8 [1.57];
    7 -> W [.15];
    8 -> W [.15];

    9 [step];
    y1 -> 9 [1];
    9 -> W [.2];
}
