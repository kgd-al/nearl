CPPN { // Solves C1 mazes (for a resolution of 7 -> Resolution input to the CPPN?)
    x0; y0; z0; x1; y1; z1; l; b;

    W [bsgm];
    L [step];
    B [id];

    // abs(x0+x1)
    0 [abs];
    x0 -> 0 [1];
    x1 -> 0 [1];

    // step(H0-1.4)
    1 [step];
    0 -> 1 [1];
    b -> 1 [-1.4];

    // abs(z0+z1)
    2 [abs];
    z0 -> 2 [1];
    z1 -> 2 [1];

    // step(H2-1.4)
    3 [step];
    2 -> 3 [1];
    b -> 3 [-1.4];
}
